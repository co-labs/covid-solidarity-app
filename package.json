{
  "name": "solidarity-app",
  "version": "1.0.0",
  "scripts": {
    "api": "cd api && npm run start",
    "api-dev": "cd api && npm run start --dev",
    "api-install": "cd api && npm install",
    "www": "cd www && npm run start",
    "www-dev": "cd www && npm run dev",
    "www-install": "cd www && npm install",
    "dev": "concurrently \"npm run api-dev\" \"npm run www-dev\"",
    "start": "concurrently \"npm run api\" \"npm run www\"",
    "install": "concurrently \"npm run api-install\" \"npm run www-install\""
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
